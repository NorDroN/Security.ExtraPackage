{
  "version": "1.0.0-*",
  "description": "ASP.NET Core middleware that enables an application to support VK's OAuth 2.0 authentication workflow.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/NorDroN/Security.ExtraPackage"
    },
    "tags": [
      "aspnetcore",
      "authentication",
      "security"
    ]
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.AspNetCore.Authentication.OAuth": "1.0.0-rc2-final"
  },
  "frameworks": {
    "net451": {},
    "netstandard1.3": {
      "imports": [
        "portable-net451+win8"
      ]
    }
  }
}